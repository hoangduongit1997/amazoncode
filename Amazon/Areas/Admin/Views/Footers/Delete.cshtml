@model Amazon.DTO.FooterDTO
@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAD.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel-heading">
            <h3>Footer Delete</h3>
        </div>
        <div class="panel-body">

            <h3>Are you sure you want to delete this?</h3>
            <div>
                <hr />
                <table class="table table-striped table-bordered table-hover">
                    <tr>
                        <th>FooterID</th>
                        <td>
                            @Html.DisplayFor(model => model.FooterID)
                        </td>
                    </tr>
                    <tr>
                        <th>Contain</th>
                        <td>
                            @Html.DisplayFor(model => model.Contain)
                        </td>
                    </tr>
                    <tr>
                        <th>Status</th>
                        <td>
                            @Html.DisplayFor(model => model.Status)
                        </td>
                    </tr>
                    <tr>
                        <th>Footer ID Type</th>
                        <td>
                            @Html.DisplayFor(model => model.FooterIDType)
                        </td>
                    </tr>
                    <tr>
                        <th>Footer Parent ID</th>
                        <td>
                            @Html.DisplayFor(model => model.FooterParentID)
                        </td>
                    </tr>
                    <tr>
                        <th>Link</th>
                        <td>
                            @Html.DisplayFor(model => model.Link)
                        </td>
                    </tr>
                    <tr>
                        <th>Target</th>
                        <td>
                            @Html.DisplayFor(model => model.Target)
                        </td>
                    </tr>
                </table>
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-default" /> |
                        @Html.ActionLink("Back to List", "Index")
                    </div>
                }
            </div>
        </div>
    </div>
</div>
